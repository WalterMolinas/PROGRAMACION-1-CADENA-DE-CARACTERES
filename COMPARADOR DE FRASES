#include <stdio.h>
#include <string.h>
/*Pedile al usuario que ingrese dos palabras. El programa debe comparar si son iguales o no y mostrar un mensaje correspondiente. strcmp()*/
	int main() {
	char palabra1[1000];
	char palabra2[1000];
	printf("Ingrese primera Frase\n");
	scanf("%s", &palabra1);
	printf("Ingrese segunda Frase\n");
	scanf("%s", &palabra2);
	if (strcmp(palabra1,palabra2)==0){
		printf("Las frases son iguales\n");
	}
	if (strcmp(palabra1,palabra2)!=0){
		printf("Las frases son diferentes\n");
	}
	return 0;
}
